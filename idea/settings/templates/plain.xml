<templateSet group="plain">
  <template name="builder" value="public $CLASS_NAME$ with$INSTANCE_VARIABLE$($INSTANCE_VARIABLE_TYPE$ value) {&#10;    this.$INSTANCE_VARIABLE$ = value;&#10;    return this;&#10;}" description="Builder method" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="className()" defaultValue="" alwaysStopAt="true" />
    <variable name="INSTANCE_VARIABLE" expression="suggestVariableName()" defaultValue="" alwaysStopAt="true" />
    <variable name="INSTANCE_VARIABLE_TYPE" expression="typeOfVariable(VAR)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
</templateSet>